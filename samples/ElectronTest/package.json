{
  "name": "devon4ng-electron-test",
  "description": "Electron with angular",
  "author": {
    "name": "devon4ng"
  },
  "version": "0.0.0",
  "main": "electron-main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "npx ng lint Devon4ngElectronTest",
    "e2e": "ng e2e",
    "electron:tsc": "tsc -p tsconfig.serve.json",
    "electron:run": "npm run electron:tsc && ng build --base-href ./ && npx electron .",
    "electron:serve": "npm run electron:tsc && npx electron . --serve",
    "electron:pack": "npm run electron:tsc && electron-builder --dir --config electron-builder.json",
    "electron:build": "npm run electron:tsc && electron-builder --config electron-builder.json build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.0",
    "@angular/common": "~12.1.0",
    "@angular/compiler": "~12.1.0",
    "@angular/core": "~12.1.0",
    "@angular/forms": "~12.1.0",
    "@angular/platform-browser": "~12.1.0",
    "@angular/platform-browser-dynamic": "~12.1.0",
    "@angular/router": "~12.1.0",
    "core-js": "^2.5.4",
    "electron-reload": "^1.5.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.0",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "12.6.0",
    "@angular-eslint/eslint-plugin-template": "12.6.1",
    "@angular-eslint/schematics": "12.6.1",
    "@angular-eslint/template-parser": "12.6.1",
    "@angular/cli": "~12.2.13",
    "@angular/compiler-cli": "~12.1.0",
    "@angular/language-service": "~12.1.0",
    "@types/jasmine": "~3.10.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.6",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.33.0",
    "electron": "^15.3.0",
    "electron-builder": "^22.13.1",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsdoc": "37.0.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "~3.10.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "typescript": "~4.3.4",
    "wait-on": "^6.0.0"
  }
}
