{
  "name": "devon4ng-electron-test",
  "description": "Electron with angular",
  "author": {
    "name": "devon4ng"
  },
  "version": "0.0.0",
  "main": "electron-main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron:run": "ng build --base-href ./ && npx electron .",
    "electron:serve": "wait-on tcp:4200 && yarn run electron:serve-tsc && npx electron . --serve",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:pack": "electron-builder --dir --config electron-builder.json",
    "electron:build": "electron-builder --config electron-builder.json build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.4",
    "@angular/common": "~10.0.4",
    "@angular/compiler": "~10.0.4",
    "@angular/core": "~10.0.4",
    "@angular/forms": "~10.0.4",
    "@angular/platform-browser": "~10.0.4",
    "@angular/platform-browser-dynamic": "~10.0.4",
    "@angular/router": "~10.0.4",
    "core-js": "^2.5.4",
    "electron-reload": "^1.5.0",
    "rxjs": "~6.6.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.3",
    "@angular/cli": "~10.0.3",
    "@angular/compiler-cli": "~10.0.4",
    "@angular/language-service": "~10.0.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.6",
    "codelyzer": "5.1.0",
    "electron": "^9.4.0",
    "electron-builder": "^22.7.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.9.7",
    "wait-on": "^5.1.0"
  }
}
